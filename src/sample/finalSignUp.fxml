<BorderPane prefHeight="800.0" prefWidth="1400.0" stylesheets="@dark.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FinalSignUp">
   <center>
      <AnchorPane prefHeight="500.0" prefWidth="400.0" style="-fx-border-color: white;" stylesheets="@dark.css">
         <children>
            <CheckBox fx:id="tech" layoutX="818.0" layoutY="300.0" mnemonicParsing="false" styleClass="radio-button" text="Technology" />
            <Text layoutX="795.0" layoutY="282.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-id" text="Interests">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <CheckBox fx:id="politics" layoutX="818.0" layoutY="328.0" mnemonicParsing="false" styleClass="radio-button" text="Politics" />
            <CheckBox fx:id="sports" layoutX="818.0" layoutY="358.0" mnemonicParsing="false" styleClass="radio-button" text="Sports" />
            <CheckBox fx:id="entertainment" layoutX="818.0" layoutY="385.0" mnemonicParsing="false" styleClass="radio-button" text="Entertainment" />
            <Button layoutX="668.0" layoutY="467.0" mnemonicParsing="false" onAction="#loadDashboard" prefHeight="39.0" prefWidth="76.0" styleClass="custom" text="FInish" />
            <Button layoutX="475.0" layoutY="389.0" mnemonicParsing="false" onAction="#selectPic" styleClass="custom" text="Upload a photo" />
            <Text layoutX="574.0" layoutY="207.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-id" text="Set up your profile" wrappingWidth="299.0">
               <font>
                  <Font size="32.0" />
               </font>
            </Text>
            <ImageView fx:id="image" fitHeight="120.0" fitWidth="155.0" layoutX="472.0" layoutY="249.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Downloads/man.png" />
               </image>
            </ImageView>
            <Text fx:id="error" layoutX="737.0" layoutY="431.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-id" textAlignment="CENTER" wrappingWidth="273.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
